{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "rangeStart": 0,
  "parser": "babel-flow",
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "preserve",
  "arrowParens": "avoid"
}
